entrypoint = "main.py"
modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "freetype", "gitFull", "glibcLocales", "lcms2", "libaom", "libde265", "libheif", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "nasm", "openjpeg", "openssl", "pkg-config", "playwright-driver", "rapidfuzz-cpp", "rustc", "taskflow", "tcl", "tk", "x265", "zlib", "nspr", "nss", "dbus", "atk", "at-spi2-atk", "at-spi2-core", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXfixes", "mesa", "libxkbcommon", "alsa-lib", "xorg.libX11", "xorg.libxcb", "xorg.libXrandr", "xorg.libXext", "cups", "expat", "libdrm", "cairo", "pango", "glib"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["uvicorn", "backend.app:app", "--host", "0.0.0.0", "--port", "5000"]
deploymentTarget = "vm"

[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0", "python_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VintedBot Connector"

[[workflows.workflow]]
name = "VintedBot Connector"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn backend.app:app --host 0.0.0.0 --port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

